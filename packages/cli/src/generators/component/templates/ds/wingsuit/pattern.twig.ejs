{#
/**
 * @file
 * <%= name %>
 *
 * See <%= name %>.wingsuit.yaml for documentation.
 */
#}

{#
  Defaults
#}

{% set attributes = attributes|default(create_attribute()) %}
{% set setting_1 = setting_1|default('value_1') %}

{#
  Classes
#}
{% set classes = [
  setting_1 == 'value_1' ? 'w-full',
] %}

{#
  Template
#}
<div {{ attributes.addClass(classes | sort | join(' ') | trim) }}>
  <div>New Wingsuit component</div>
  <div>Field variable: <code>{{ field_1 }}</code></div>
  <div>Setting variable: <code>{{ setting_1 }}</code></div>
</div>
